include "/usr/share/X11/locale/en_US.UTF-8/Compose"

# Punctuation
<Multi_key> <Cyrillic_be> <Cyrillic_be> : "«" U00AB # RUSSIAN CLASSIC QUOTE OPEN
<Multi_key> <Cyrillic_yu> <Cyrillic_yu> : "»" U00BB # RUSSIAN CLASSIC QUOTE CLOSE
<Multi_key> <Cyrillic_BE> <Cyrillic_BE> : "„" U201E # RUSSIAN PAW QUOTE OPEN
<Multi_key> <Cyrillic_YU> <Cyrillic_YU> : "“" U201C # RUSSIAN PAW QUOTE CLOSE
<Multi_key> <minus> <minus>             : "—" U2014 # LONG MINUS

# Symbols
<Multi_key> <apostrophe> <apostrophe>   : "́" U0301 # LIGHT ACCENT
<Multi_key> <exclam> <equal>            : "≠" U2260 # NOT EQUAL TO

# Arrows
<Multi_key> <l> <l>                     : "←" U2190 # LEFTWARDS ARROW
<Multi_key> <u> <u>                     : "↑" U2191 # UPWARDS ARROW
<Multi_key> <r> <r>                     : "→" U2192 # RIGHTWARDS ARROW
<Multi_key> <d> <d>                     : "↓" U2193 # DOWNWARDS ARROW

# Rouble
<Multi_key> <p> <p>                     : "₽" U20bd # RUBLE-CURRENCY SIGN
<Multi_key> <Cyrillic_er> <Cyrillic_er> : "₽" U20bd # RUBLE-CURRENCY SIGN
